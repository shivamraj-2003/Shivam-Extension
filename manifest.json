{
    "manifest_version": 3,
    "name": "Tab Title Viewer",
    "version": "1.0",
    "description": "A Chrome extension to display the title of the current tab.",
    "permissions": [ "tabs" ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "chrome.png",
        "48": "chrome.png",
        "128": "chrome.png"
    }


}
